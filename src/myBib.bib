@article{tropshaBestPracticesQSAR2010,
  title = {Best {{Practices}} for {{QSAR Model Development}}, {{Validation}}, and {{Exploitation}}.},
  author = {Tropsha, Alexander},
  year = {2010},
  month = jul,
  journal = {Molecular Informatics},
  volume = {29},
  pages = {476--488},
  doi = {10.1002/minf.201000061},
  abstract = {After nearly five decades ``in the making'', QSAR modeling has established itself as one of the major computational molecular modeling methodologies. As any mature research discipline, QSAR modeling can be characterized by a collection of well defined protocols and procedures that enable the expert application of the method for exploring and exploiting ever growing collections of biologically active chemical compounds. This review examines most critical QSAR modeling routines that we regard as best practices in the field. We discuss these procedures in the context of integrative predictive QSAR modeling workflow that is focused on achieving models of the highest statistical rigor and external predictive power. Specific elements of the workflow consist of data preparation including chemical structure (and when possible, associated biological data) curation, outlier detection, dataset balancing, and model validation. We especially emphasize procedures used to validate models, both internally and externally, as well as the need to define model applicability domains that should be used when models are employed for the prediction of external compounds or compound libraries. Finally, we present several examples of successful applications of QSAR models for virtual screening to identify experimentally confirmed hits.},
  pmid = {27463326},
  annotation = {MAG ID: 2128245586},
  file = {/home/ajensen123@ad.ufl.edu/references/docs/Tropsha-2010_Best Practices for QSAR Model Development, Validation, and Exploitation/Tropsha_2010_Best Practices for QSAR Model Development, Validation, and Exploitation.pdf}
}

@article{tsaiVolumetricModelbased2D2010,
  ids = {tsaiVolumetricModelbased2D2010a},
  title = {A Volumetric Model-Based {{2D}} to {{3D}} Registration Method for Measuring Kinematics of Natural Knees with Single-Plane Fluoroscopy: {{2D}}/{{3D}} Registration Method for Measuring Natural Knee Kinematics},
  shorttitle = {A Volumetric Model-Based {{2D}} to {{3D}} Registration Method for Measuring Kinematics of Natural Knees with Single-Plane Fluoroscopy},
  author = {Tsai, Tsung-Yuan and Lu, Tung-Wu and Chen, Chung-Ming and Kuo, Mei-Ying and Hsu, Horng-Chaung},
  year = {2010},
  month = feb,
  journal = {Medical Physics},
  volume = {37},
  number = {3},
  pages = {1273--1284},
  issn = {00942405},
  doi = {10.1118/1.3301596},
  urldate = {2021-02-17},
  abstract = {Purpose: Accurate measurement of the three-dimensional ͑3D͒ rigid body and surface kinematics of the natural human knee is essential for many clinical applications. Existing techniques are limited either in their accuracy or lack more realistic experimental evaluation of the measurement errors. The purposes of the study were to develop a volumetric model-based 2D to 3D registration method, called the weighted edge-matching score ͑WEMS͒ method, for measuring natural knee kinematics with single-plane fluoroscopy to determine experimentally the measurement errors and to compare its performance with that of pattern intensity ͑PI͒ and gradient difference ͑GD͒ methods. Methods: The WEMS method gives higher priority to matching of longer edges of the digitally reconstructed radiograph and fluoroscopic images. The measurement errors of the methods were evaluated based on a human cadaveric knee at 11 flexion positions. Results: The accuracy of the WEMS method was determined experimentally to be less than 0.77 mm for the in-plane translations, 3.06 mm for out-of-plane translation, and 1.13{\textdegree} for all rotations, which is better than that of the PI and GD methods. Conclusions: A new volumetric model-based 2D to 3D registration method has been developed for measuring 3D in vivo kinematics of natural knee joints with single-plane fluoroscopy. With the equipment used in the current study, the accuracy of the WEMS method is considered acceptable for the measurement of the 3D kinematics of the natural knee in clinical applications. {\textcopyright} 2010 American Association of Physicists in Medicine. ͓DOI: 10.1118/1.3301596͔},
  langid = {english},
  keywords = {,fluoroscopy,Kinematics,Knee,Natural Knee,Single Plane,Volumetric},
  file = {/home/ajensen123@ad.ufl.edu/references/docs/Tsai et al-2010_A volumetric model-based 2D to 3D registration method for measuring kinematics/Tsai et al. - 2010 - A volumetric model-based 2D to 3D registration met.pdf;/home/ajensen123@ad.ufl.edu/references/docs/Tsai et al-2010_A volumetric model-based 2D to 3D registration method for measuring kinematics/Tsai et al. - 2010 - A volumetric model-based 2D to 3D registration met2.pdf}
}

@article{tsuchiyaRelationshipPreoperativeGoutallier2022,
  title = {The {{Relationship}} between {{Preoperative Goutallier Stage}} and {{Retear Rates}} Following {{Posterosuperior Rotator Cuff Repair}}: {{A Systematic Review}}},
  author = {Tsuchiya, Saho and {Saho Tsuchiya} and {Aaron J. Bois} and {Aaron J. Bois} and Matthewson, Graeme and {Graeme Matthewson} and {Shin Oiwa} and {Shin Oiwa} and {Kristie D. More} and {Kristie D. More} and Lo, Ian K.Y. and {Ian K.Y. Lo}},
  year = {2022},
  month = oct,
  journal = {Journal of Shoulder and Elbow Surgery},
  doi = {10.1016/j.jse.2022.09.011},
  abstract = {An association between higher preoperative Goutallier stage and higher retear rates following primary rotator cuff repairs has been previously reported. However, there are few reviews which have described clear retear rates for each repaired tendon classified according to preoperative Goutallier stage. The purpose of this study was therefore to systematically review the literature on the relationship between preoperative Goutallier stage and retear rates and provide predictable retear rates following primary repair of posterosuperior rotator cuff tears.A systematic literature review was performed in compliance with the PRISMA guidelines and checklist utilizing PubMed, MEDLINE and Cochrane Library. English-language studies of Level I through IV evidence examining the clinical results of primarily repaired posterosuperior rotator cuff tears as a function of Goutallier stage using MRI were included. The primary outcome of interest was retear rates according to preoperative Goutallier stage. As a secondary outcome, rotator cuff tears were divided into two subgroups (Goutallier stage 0-2 and 3-4) and retear rates were assessed between the two subgroups. A random effects model with binomial within-study variance was used for both outcomes.Nine studies (687 shoulders) satisfied all inclusion criteria and reported sufficient data for statistical analysis. In the supraspinatus muscle group, rotator cuff muscles with preoperative Goutallier stages of 0, 1 and 2 had retear rates of 19.1\%, 27.8\% and 33.5\%, respectively with no significant differences between each category. In contrast, when preoperative Goutallier stage of 3 and 4 existed, significant differences compared to Goutallier 0, 1 and 2 were observed with a retear rate of 74.1\% and 78.5\%, respectively. In the infraspinatus muscle group, the retear rates following rotator cuff repairs were 20.0\%, 32.1\% and 35.1\% in Goutallier 0, 1 and 2, respectively, with no significant differences between each category. In Goutallier 3 and 4, the rate was 76.6\% and 100.0\%, respectively, with significant differences compared to Goutallier 0, 1 and 2.The results of this study have clearly demonstrated that retear rates following surgical repair of the rotator cuff increased in proportion to the preoperative Goutallier stage in both the supraspinatus and infraspinatus muscles.},
  pmid = {36252788},
  annotation = {MAG ID: 4306318706 S2ID: cc44e884ce6ae0f7efe70e6c40d6b18f27efed80},
  file = {/home/ajensen123@ad.ufl.edu/references/docs/Tsuchiya et al-2022_The Relationship between Preoperative Goutallier Stage and Retear Rates/Tsuchiya et al_2022_The Relationship between Preoperative Goutallier Stage and Retear Rates.pdf}
}

