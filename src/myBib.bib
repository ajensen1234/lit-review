@article{palsisCurrentTrendsUse2018,
  title = {Current Trends in the Use of Shoulder Arthroplasty in the {{United States}}},
  author = {Palsis, John A and Simpson, Kit N and Matthews, J Hunter and Traven, Sophia and Eichinger, Josef K and Friedman, Richard J},
  year = {2018},
