@article{tornaiFastDRRGeneration2012,
  title = {Fast {{DRR}} Generation for {{2D}} to {{3D}} Registration on {{GPUs}}},
  author = {Tornai, G{\'a}bor J{\'a}nos and Cserey, Gyoergy and Pappas, Ion},
  year = {2012},
  month = aug,
  journal = {Medical Physics},
  volume = {39},
  number = {8},
  pages = {4795--4799},
  doi = {10.1118/1.4736827},
  abstract = {Purpose : The generation of digitally reconstructedradiographs(DRRs) is the most time consuming step on the CPU in intensity based two-dimensional x-ray to three-dimensional (CT or 3D rotational x-ray)medical image registration, which has application in several image guided interventions. This work presents optimized DRR rendering on graphical processor units (GPUs) and compares performance achievable on four commercially available devices. Methods : A ray-cast based DRR rendering was implemented for a 512 {\texttimes} 512 {\texttimes} 72 CT volume. The block size parameter was optimized for four different GPUs for a region of interest (ROI) of 400 {\texttimes} 225 pixels with different sampling ratios (1.1\%{\textendash}9.1\% and 100\%). Performance was statistically evaluated and compared for the four GPUs. The method and the block size dependence were validated on the latest GPU for several parameter settings with a public gold standard dataset (512 {\texttimes} 512 {\texttimes} 825 CT) for registration purposes. Results : Depending on the GPU, the full ROI is rendered in 2.7{\textendash}5.2 ms. If sampling ratio of 1.1\%{\textendash}9.1\% is applied, execution time is in the range of 0.3{\textendash}7.3 ms. On all GPUs, the mean of the execution time increased linearly with respect to the number of pixels if sampling was used. Conclusions : The presented results outperform other results from the literature. This indicates that automatic 2D to 3D registration, which typically requires a couple of hundred DRR renderings to converge, can be performed quasi on-line, in less than a second or depending on the application and hardware in less than a couple of seconds. Accordingly, a whole new field of applications is opened for image guided interventions, where the registration is continuously performed to match the real-time x-ray.},
  pmid = {22894404},
  annotation = {MAG ID: 2027735590},
  file = {/home/ajensen123@ad.ufl.edu/references/docs/Tornai et al-2012_Fast DRR generation for 2D to 3D registration on GPUs/Tornai et al. - 2012 - Fast DRR generation for 2D to 3D registration on G.pdf}
}

