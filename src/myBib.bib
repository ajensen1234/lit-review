  author = {Fritscher, Karl D. and {Benedikt Schuler} and Schuler, Benedikt and Link, Thomas M. and Eckstein, Felix and Suhm, Norbert and H{\"a}nni, Markus and Hengg, Clemens and Schubert, Rainer},
