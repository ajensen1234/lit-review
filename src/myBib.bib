@article{liang-yiwuCurcuminAttenuatesAdipogenesis2019,
  title = {Curcumin {{Attenuates Adipogenesis}} by {{Inducing Preadipocyte Apoptosis}} and {{Inhibiting Adipocyte Differentiation}}},
  author = {{Liang-Yi Wu} and Wu, Liang-Yi and Chen, Chien-Wei and Chen, Luen-Kui and Chen, Luen-Kui and Chou, Hsiang-Yun and Chang, Chih-Ling and Chang, Chih-Ling and Juan, Chi-Chang},
  year = {2019},
  month = sep,
  journal = {Nutrients},
  volume = {11},
  number = {10},
  pages = {2307},
  doi = {10.3390/nu11102307},
  abstract = {Patients with metabolic syndrome are at an increased risk of developing type 2 diabetes and cardiovascular diseases. The principal risk factor for development of metabolic syndrome is obesity, defined as a state of pathological hyperplasia or/and hypertrophy of adipose tissue. The number of mature adipocytes is determined by adipocyte differentiation from preadipocytes. The purpose of the present study is to investigate the effects of curcumin on adipogenesis and the underlying mechanism. To examine cell toxicity of curcumin, 3T3-L1 preadipocytes were treated with 0{\textendash}50 {\textmu}M curcumin for 24, 48, or 72 h, then cell viability was measured using the MTT (3-(4,5-dimethylthiazol-2-yl)-2,5-diphenyltetrazolium bromide) assay. The effect of curcumin on the cell cycle was determined by flow cytometry. Curcumin-induced cell apoptosis was determined by the TUNEL assay and curcumin-induced caspase activation was measured by immunoblotting. The effect of curcumin on adipocyte differentiation was determined by measuring mitotic clonal expansion (MCE), expression of adipogenic transcription factors, and lipid accumulation. Results showed the viability of preadipocytes was significantly decreased by treatment with 30 {\textmu}M curcumin, a concentration that caused apoptosis in preadipocytes, as assessed by the TUNEL assay, and caused activation of caspases 8, 9, and 3. A non-cytotoxic dose of curcumin (15 {\textmu}M) inhibited MCE, downregulated the expression of PPAR{$\gamma$} and C/EBP{$\alpha$}, prevented differentiation medium-induced {$\beta$}-catenin downregulation, and decreased the lipid accumulation in 3T3-L1 adipocytes. In conclusion, our data show that curcumin can induce preadipocyte apoptosis and inhibit adipocyte differentiation, leading to suppression of adipogenesis.},
  pmcid = {6836120},
  pmid = {31569380},
  annotation = {MAG ID: 2977012963 S2ID: 0510e739738f32d3413ecfba3bc4f6ca0e3d9fb3},
  file = {/home/ajensen123@ad.ufl.edu/references/docs/Liang-Yi Wu et al-2019_Curcumin Attenuates Adipogenesis by Inducing Preadipocyte Apoptosis and/Liang-Yi Wu et al_2019_Curcumin Attenuates Adipogenesis by Inducing Preadipocyte Apoptosis and.pdf}
}

@misc{liangShapeGraphMatchingNetwork2023,
  title = {Shape-{{Graph Matching Network}} ({{SGM-net}}): {{Registration}} for {{Statistical Shape Analysis}}},
  shorttitle = {Shape-{{Graph Matching Network}} ({{SGM-net}})},
  author = {Liang, Shenyuan and Segundo, Mauricio Pamplona and Aakur, Sathyanarayanan N. and Sarkar, Sudeep and Srivastava, Anuj},
  year = {2023},
  month = aug,
  number = {arXiv:2308.06869},
  eprint = {2308.06869},
  primaryclass = {cs, stat},
  publisher = {{arXiv}},
  urldate = {2023-11-15},
  abstract = {This paper focuses on the statistical analysis of shapes of data objects called shape graphs, a set of nodes connected by articulated curves with arbitrary shapes. A critical need here is a constrained registration of points (nodes to nodes, edges to edges) across objects. This, in turn, requires optimization over the permutation group, made challenging by differences in nodes (in terms of numbers, locations) and edges (in terms of shapes, placements, and sizes) across objects. This paper tackles this registration problem using a novel neural-network architecture and involves an unsupervised loss function developed using the elastic shape metric for curves. This architecture results in (1) state-of-the-art matching performance and (2) an order of magnitude reduction in the computational cost relative to baseline approaches. We demonstrate the effectiveness of the proposed approach using both simulated data and real-world 2D and 3D shape graphs. Code and data will be made publicly available after review to foster research.},
  archiveprefix = {arxiv},
  langid = {english},
  keywords = {Computer Science - Computer Vision and Pattern Recognition,Statistics - Machine Learning},
  file = {/home/ajensen123@ad.ufl.edu/references/docs/Liang et al-2023_Shape-Graph Matching Network (SGM-net)/Liang et al. - 2023 - Shape-Graph Matching Network (SGM-net) Registrati.pdf}
}

@article{liAnteriorCruciateLigament2006,
  title = {Anterior {{Cruciate Ligament Deficiency Alters}} the {{In Vivo Motion}} of the {{Tibiofemoral Cartilage Contact Points}} in {{Both}} the {{Anteroposterior}} and {{Mediolateral Directions}}:},
  shorttitle = {Anterior {{Cruciate Ligament Deficiency Alters}} the {{In Vivo Motion}} of the {{Tibiofemoral Cartilage Contact Points}} in {{Both}} the {{Anteroposterior}} and {{Mediolateral Directions}}},
  author = {Li, Guoan and Moses, Jeremy M. and Papannagari, Ramprasad and Pathare, Neil P. and DeFrate, Louis E. and Gill, Thomas J.},
  year = {2006},
  month = aug,
  journal = {The Journal of Bone \& Joint Surgery},
  volume = {88},
  number = {8},
  pages = {1826--1834},
  issn = {0021-9355},
  doi = {10.2106/JBJS.E.00539},
  urldate = {2021-09-09},
  abstract = {Background: Quantifying the effects of anterior cruciate ligament deficiency on joint biomechanics is critical in order to better understand the mechanisms of joint degeneration in anterior cruciate ligament-deficient knees and to improve the surgical treatment of anterior cruciate ligament injuries. We investigated the changes in position of the in vivo tibiofemoral articular cartilage contact points in anterior cruciate ligament-deficient and intact contralateral knees with use of a newly developed dual orthogonal fluoroscopic and magnetic resonance imaging technique. Methods: Nine patients with an anterior cruciate ligament rupture in one knee and a normal contralateral knee were recruited. Magnetic resonance images were acquired for both the intact and anterior cruciate ligament-deficient knees to construct computer knee models of the surfaces of the bone and cartilage. Each patient performed a singleleg weight-bearing lunge as images were recorded with use of a dual fluoroscopic system at full extension and at 15{\textdegree}, 30{\textdegree}, 60{\textdegree}, and 90{\textdegree} of flexion. The in vivo knee position at each flexion angle was then reproduced with use of the knee models and fluoroscopic images. The contact points were defined as the centroids of the areas of intersection of the tibial and femoral articular cartilage surfaces. Results: The contact points moved not only in the anteroposterior direction but also in the mediolateral direction in both the anterior cruciate ligament-deficient and intact knees. In the anteroposterior direction, the contact points in the medial compartment of the tibia were more posterior in the anterior cruciate ligament-deficient knees than in the intact knees at full extension and 15{\textdegree} of flexion (p {$<$} 0.05). No significant differences were observed with regard to the anteroposterior motion of the contact points in the lateral compartment of the tibia. In the mediolateral direction, there was a significant lateral shift of the contact points in the medial compartment of the tibia toward the medial tibial spine between full extension and 60{\textdegree} of flexion (p {$<$} 0.05). The contact points in the lateral compartment of the tibia shifted laterally, away from the lateral tibial spine, at 15{\textdegree} and 30{\textdegree} of flexion (p {$<$} 0.05). Conclusions: In the presence of anterior cruciate ligament injury, the contact points shift both posteriorly and laterally on the surface of the tibial plateau. In the medial compartment, the contact points shift toward the medial tibial spine, a region where degeneration is observed in patients with chronic anterior cruciate ligament injuries.},
  langid = {english},
  file = {/home/ajensen123@ad.ufl.edu/references/docs/Li et al-2006_Anterior Cruciate Ligament Deficiency Alters the In Vivo Motion of the/Li et al. - 2006 - Anterior Cruciate Ligament Deficiency Alters the I.pdf}
}

@article{liDigitallyReconstructedRadiograph2006,
  title = {Digitally Reconstructed Radiograph Generation by an Adaptive {{Monte Carlo}} Method},
  author = {Li, Xiaoliang and Yang, Jun and Yang, Jie and {Jie Yang} and Yang, Jie and Zhu, Yuemin},
  year = {2006},
  month = jun,
  journal = {Physics in Medicine and Biology},
  volume = {51},
  number = {11},
  pages = {2745--2752},
  doi = {10.1088/0031-9155/51/11/004},
  abstract = {Digitally reconstructed radiograph (DRR) generation is an important step in several medical imaging applications such as 2D{\textendash}3D image registration, where the generation of DRR is a rate-limiting step. We present a novel DRR generation technique, called the adaptive Monte Carlo volume rendering (AMCVR) algorithm. It is based on the conventional Monte Carlo volume rendering (MCVR) technique that is very efficient for rendering large medical datasets. In contrast to the MCVR, the AMCVR does not produce sample points by sampling directly in the entire volume domain. Instead, it adaptively divides the entire volume domain into sub-domains using importance separation and then performs sampling in these sub-domains. As a result, the AMCVR produces almost the same image quality as that obtained with the MCVR while only using half samples, and increases projection speed by a factor of 2. Moreover, the AMCVR is suitable for fast memory addressing, which further improves processing speed. Independent of the size of medical datasets, the AMCVR allows for achieving a frame rate of about 15 Hz on a 2.8 GHz Pentium 4 PC while generating reasonably good quality DRR.},
  pmid = {16723763},
  annotation = {MAG ID: 1967729905},
  file = {/home/ajensen123@ad.ufl.edu/references/docs/Li et al-2006_Digitally reconstructed radiograph generation by an adaptive Monte Carlo method/Li et al. - 2006 - Digitally reconstructed radiograph generation by a.pdf;/home/ajensen123@ad.ufl.edu/references/docs/Li et al-2006_Digitally reconstructed radiograph generation by an adaptive Monte Carlo method/Li et al. - 2006 - Digitally reconstructed radiograph generation by a2.pdf}
}

