@article{banksRationaleResultsFixedBearing2019,
  title = {Rationale and {{Results}} for {{Fixed-Bearing Pivoting Designs}} in {{Total Knee Arthroplasty}}},
  author = {Banks, Scott A. and Deckard, Evan and Hodge, W. Andrew and Meneghini, R. Michael},
