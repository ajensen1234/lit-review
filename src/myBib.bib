@article{satoshiyamaguchiVivoKinematicsTwocomponent2011,
  title = {In Vivo Kinematics of Two-Component Total Ankle Arthroplasty during Non-Weightbearing and Weightbearing Dorsiflexion/Plantarflexion.},
  author = {{Satoshi Yamaguchi} and {Yasuhito Tanaka} and {Shinichi Kosugi} and {Yoshinori Takakura} and {Takahisa Sasho} and {Scott A. Banks}},
  year = {2011},
  journal = {Journal of Biomechanics},
  doi = {10.1016/j.jbiomech.2011.02.078},
  abstract = {Abstract   Relatively high rates of loosening and implant failure have been reported after total ankle arthroplasty, especially in first and second generation implants. Abnormal kinematics and incongruency of the articular surface may cause increased loads applied to the implant with concomitant polyethylene wear, resulting in loosening and implant failure. The purpose of this study was to measure three-dimensional kinematics of two-component total ankle arthroplasty during non-weightbearing and weightbearing activities, and to investigate incongruency of the articular surfaces during these activities. Forty-seven patients with a mean age of 71 years were enrolled. Radiographs were taken at non-weightbearing maximal dorsiflexion and plantarflexion, and weightbearing maximal dorsiflexion, plantarflexion, and neutral position. 3D{\textendash}2D model-image registration was performed using the radiographs and the three-dimensional implant models, and three-dimensional joint angles were determined. The implanted ankles showed 18.1{$\pm$}8.6{\textdegree} (mean{$\pm$}standard deviation) of plantarflexion, 0.1{$\pm$}0.7{\textdegree} of inversion, 1.2{$\pm$}2.0{\textdegree} of internal rotation, and 0.8{$\pm$}0.6mm of posterior translation of the talar component in the non-weightbearing activity, and 17.8{$\pm$}7.5{\textdegree} of plantarflexion, 0.4{$\pm$}0.5{\textdegree} of inversion, 1.8{$\pm$}2.0{\textdegree} of internal rotation, and 0.7{$\pm$}0.5mm of posterior translation in the weightbearing activity. There were no significant differences between the non-weightbearing and weightbearing kinematics except for the plantarflexion angle. Incongruency of the articular surface occurred in more than 75\% of the ankles. Our observations will provide useful data against which kinematics of other implant designs, such as three-component total ankle arthroplasty, can be compared.},
  pmid = {21392769},
  annotation = {MAG ID: 2043397945},
  file = {/home/ajensen123@ad.ufl.edu/references/docs/Satoshi Yamaguchi et al-2011_In vivo kinematics of two-component total ankle arthroplasty during/Satoshi Yamaguchi et al. - 2011 - In vivo kinematics of two-component total ankle ar.pdf}
}

