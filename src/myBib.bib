@article{chengDeepLearningFully2021,
  title = {Deep {{Learning}} for {{Fully Automated Prediction}} of {{Overall Survival}} in {{Patients}} with {{Oropharyngeal Cancer Using FDG-PET Imaging}}.},
  author = {Cheng, Nai-Ming and Yao, Jiawen and Cai, Jinzheng and Ye, Xianghua and Zhao, Shilin and Zhao, Shilin and {Shilin Zhao} and {Shilin Zhao} and {Shilin Zhao} and Zhao, Kui and Zhou, Wenlan and {Isabella Nogues} and {Isabella Nogues} and Nogues, Isabella and {Isabella Nogues} and {Isabella Nogues} and {Yuankai Huo} and Huo, Yuankai and Liao, Chun-Ta and Ng, Shu-Hang and Wang, Hung-Ming and {Chien-Yu Lin} and Lin, Chien-Yu and Lee, Li-Yu and Xiao, Jing and Lu, Le and {Ling Zhang} and Zhang, Ling and Yen, Tzu-Chen},
  year = {2021},
  month = may,
  journal = {Clinical Cancer Research},
  volume = {27},
  number = {14},
  pages = {3948--3959},
  doi = {10.1158/1078-0432.ccr-20-4935},
  abstract = {Purpose  Accurate prognostic stratification of patients with oropharyngeal squamous cell carcinoma (OPSCC) is crucial. We developed an objective and robust deep learning-based fully-automated tool called the DeepPET-OPSCC biomarker for predicting overall survival (OS) in OPSCC using [18F]fluorodeoxyglucose PET imaging.  Experimental design  The DeepPET-OPSCC prediction model was built and tested internally on a discovery cohort (n = 268) by integrating five convolutional neural network models for volumetric segmentation and ten models for OS prognostication. Two external test cohorts were enrolled - the first based on the Cancer Imaging Archive (TCIA) database (n = 353) and the second being a clinical deployment cohort (n = 31) - to assess the DeepPET-OPSCC performance and goodness of fit.  Results  After adjustment for potential confounders, DeepPET-OPSCC was found to be an independent predictor of OS in both discovery and TCIA test cohorts (HR = 2.07; 95\% CI 1.31-3.28 and HR = 2.39; 1.38-4.16; both P = 0.002). The tool also revealed good predictive performance, with a c-index of 0.707 (95\% CI 0.658-0.757) in the discovery cohort, 0.689 (0.621-0.757) in the TCIA test cohort, and 0.787 (0.675-0.899) in the clinical deployment test cohort; the average time taken was 2 min for calculation per exam. The integrated nomogram of DeepPET-OPSCC and clinical risk factors significantly outperformed the clinical model (AUC at 5 years: 0.801 [95\% CI 0.727-0.874] versus 0.749 [0.649-0.842]; P = 0.031) in the TCIA test cohort.  Conclusions  DeepPET-OPSCC achieved an accurate OS prediction in patients with OPSCC and enabled an objective, unbiased, and rapid assessment for OPSCC prognostication.},
  pmid = {33947697},
  annotation = {MAG ID: 3157955874},
  file = {/home/ajensen123@ad.ufl.edu/references/docs/Cheng et al-2021_Deep Learning for Fully Automated Prediction of Overall Survival in Patients/Cheng et al_2021_Deep Learning for Fully Automated Prediction of Overall Survival in Patients.pdf}
}

@article{chengHypolipidemicEffectsLinalool2018,
  title = {Hypolipidemic Effects of {{S-}}(+)-Linalool and Essential Oil from {{Cinnamomum}} Osmophloeum Ct. Linalool Leaves in Mice.},
  author = {Cheng, Bing-Ho and Sheen, Lee-Yan and Chang, Shang-Tzen},
  year = {2018},
  month = jan,
  journal = {Journal of Traditional and Complementary Medicine},
  volume = {8},
  number = {1},
  pages = {46--52},
  doi = {10.1016/j.jtcme.2017.02.002},
  abstract = {Abstract   Cinnamomum osmophloeum (土肉桂 t{\v u} rou gui) ct. linalool is one of the chemotypes of the indigenous cinnamon in Taiwan. S-(+)-linalool is the major constituent of leaf essential oil (LEO) of C.osmophloeum ct. linalool. This study aimed to investigate its physiological effects including body weight changes, blood biochemical values, and histopathological changes in mice. The mice were treated with LEO, S-(+)-linalool, and R-(-)-linalool. Results demonstrated similar physiological changes in mice treated with LEO and S-(+)-linalool, but significantly different effects in the body weight, TG, TC and blood glucose of R-(-)-linalool group. S-(+)-linalool-treated mice gained less weight and had significant decrease in blood triglyceride levels. No histopathological changes were observed in livers, kidneys, and spleens of S-(+)-linalool-treated mice. Furthermore, there were no significant differences in aspartate aminotransferase (AST) and alanine aminotransferase (ALT) levels between S-(+)-linalool-treated mice and the control group. In addition, LEO and S-(+)-linalool significantly inhibited lipid accumulation through down-regulation of 3T3-L1 adipocyte differentiation. Taken together, the results show that LEO and S-(+)-linalool from C.osmophloeum ct. linalool can contribute to body weight management without harmful side effects.},
  pmcid = {5755959},
  pmid = {29321988},
  annotation = {MAG ID: 2592998718 S2ID: 8222c6b2530a39c3878114c9ba00670201c47804},
  file = {/home/ajensen123@ad.ufl.edu/references/docs/Cheng et al-2018_Hypolipidemic effects of S-(+)-linalool and essential oil from Cinnamomum/Cheng et al_2018_Hypolipidemic effects of S-(+)-linalool and essential oil from Cinnamomum.pdf}
