  title = {In Vivo Measurement of 3-{{D}} Skeletal Kinematics from Sequences of Biplane Radiographs: {{Application}} to Knee Kinematics},
  shorttitle = {In Vivo Measurement of 3-{{D}} Skeletal Kinematics from Sequences of Biplane Radiographs},
