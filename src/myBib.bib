@article{tangDeepLearningImaging2020,
  title = {Deep {{Learning}} of {{Imaging Phenotype}} and {{Genotype}} for {{Predicting Overall Survival Time}} of {{Glioblastoma Patients}}},
  author = {Tang, Zhenyu and Xu, Yuyun and Xu, Yuyun and {Lei Jin} and Jin, Lei and {Abudumijiti Aibaidula} and Aibaidula, Abudumijiti and {Junfeng Lu} and Lu, Junfeng and Jiao, Zhicheng and Wu, Jinsong and {Jinsong Wu} and {Jinsong Wu} and Wu, Jinsong and Zhang, Han and Shen, Dinggang},
  year = {2020},
