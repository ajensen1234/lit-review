  file = {/home/ajensen123@ad.ufl.edu/references/docs/Ishii et al-2007_Effect of knee laxity on in vivo kinematics of meniscal-bearing knee prostheses/Ishii et al. - 2007 - Effect of knee laxity on in vivo kinematics of men.pdf}
}

@article{ivesterReconfigurableHighSpeedStereoRadiography2015,
  title = {A {{Reconfigurable High-Speed Stereo-Radiography System}} for {{Sub-Millimeter Measurement}} of {{In Vivo Joint Kinematics}}},
