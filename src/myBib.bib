@article{kapteinEvaluationThreePose2004,
  title = {Evaluation of Three Pose Estimation Algorithms for Model-Based {{Roentgen}} Stereophotogrammetric Analysis},
  author = {Kaptein, B L and R Valstar, E and Stoel, B C and Rozing, P M and Reiber, J H C},
  year = {2004},
