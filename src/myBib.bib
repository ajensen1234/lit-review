@inproceedings{duboisdaischeImprovedArticulatedRegistration2005,
  title = {An {{Improved Articulated Registration Method For Neck Images}}},
  booktitle = {2005 {{IEEE Engineering}} in {{Medicine}} and {{Biology}} 27th {{Annual Conference}}},
